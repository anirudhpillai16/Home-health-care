PREFIX ds: <http://data.medicare.gov/resource/tee5-ixt5/>

SELECT ?state ?star_rating ?checked_for_pain ?treated_for_pain
WHERE
 {    
 ?x ds:state ?state .
 ?x ds:quality_of_patient_care_star_rating ?star_rating .
 ?x ds:how_often_the_home_health_team_checked_patients_for_pain ?checked_for_pain .
 ?x ds:how_often_the_home_health_team_treated_their_patients_pain ?treated_for_pain .
   }